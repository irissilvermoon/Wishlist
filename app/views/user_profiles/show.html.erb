<h1><%= current_user.username %> Profile</h1>
<br />
<blockquote class="pull-right">
  <div class="span4">
    <p>
     <li><%= link_to "Invite User", new_watcher_path %></li>
              <li><%= link_to "Watchers", watchers_path %></li>
              <li><%= link_to "Watching", watched_users_path %></li>
            </p>
  </div>
</blockquote>

<address>
  <p>
    <b>Address:</b>
  </p>
  <p>
    <%= @user_profile.name %>
  </p>
  <p>
    <%= @user_profile.address1 %>
  </p>
  <p>
    <%= @user_profile.address2 %>
  </p>
  <p>
    <%= @user_profile.city %>
    <%= @user_profile.state %>
    <%= @user_profile.ZIP %>
  </p>
  <p>
    <%= @user_profile.country %>
  </p>
</address>


<td><%= link_to 'Edit Profile', edit_user_profile_path, :class => "btn-primary btn-small" %></td>